{
  "name": "@sensorfactdev/sf-i18n",
  "version": "1.0.2",
  "description": "Internationalisation client for translations managed in GraphQL",
  "repository": {
    "type": "git",
    "url": "git@github.com:Sensorfactdev/sf-i18n.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "test": "jest --coverage",
    "test-travis": "yarn test && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "i18n Tests",
    "output": "./reports/junit.xml"
  },
  "author": "Ramon Gebben <ramon@sensorfact.nl>",
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "jest": "^21.2.1",
    "jest-junit": "^3.1.0"
  },
  "dependencies": {
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "intl-relativeformat": "^1.3.0"
  },
  "main": "src/index.js"
}
