{
  "name": "@sensorfactdev/sf-i18n",
  "version": "0.0.34",
  "description": "Sensorfacts i18n module",
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.org:sensorfactdev/sf-i18n.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublish": ". ./.scripts/prepublish.sh",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "download:translations": "babel-node ./.scripts/download-translations.js download -S 1qDDH0aTg089GTEevK1hKLDcqNzKugMkH3OzK2uTpIkA",
    "update:translations": "npm run download:translations && git add . && git commit -m \"Updated translations\" && npm version patch"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-react-app": "^0.2.1",
    "chai": "^3.5.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "git-url-parse": "^6.0.1",
    "mocha": "^3.0.2",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "chokidar": "^1.6.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "intl-relativeformat": "^1.3.0",
    "jsdom": "^9.8.0",
    "lodash": "^4.17.2",
    "tabletop": "^1.4.3",
    "yargs": "^6.2.0"
  },
  "main": "dist/index.js",
  "engines": {
    "npm": "^3.0.0"
  }
}
